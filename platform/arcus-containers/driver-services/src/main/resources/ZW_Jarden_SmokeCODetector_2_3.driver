   //**********************************************************************************
   //CO REFLEXES
   //**********************************************************************************
   match reflex {
      on alarm.report, alarmtype: AlarmReportV2.ZWAVE_ALARM_TYPE_CO, alarmlevel: AlarmReportV2.ZWAVE_ALARM_STATUS_ON
      set CarbonMonoxide.co, CarbonMonoxide.CO_DETECTED
   }

   match reflex {
      on alarm.report, alarmtype:AlarmReportV2.ZWAVE_ALARM_TYPE_CO, alarmlevel:AlarmReportV2.ZWAVE_ALARM_STATUS_OFF
      set CarbonMonoxide.co, CarbonMonoxide.CO_SAFE
   }

