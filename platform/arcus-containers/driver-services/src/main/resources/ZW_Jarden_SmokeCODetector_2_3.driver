      on alarm.report, alarmtype: AlarmReportV2.ZWAVE_ALARM_TYPE_CO, alarmlevel: 0xFD
      on alarm.report, alarmtype: AlarmReportV2.ZWAVE_ALARM_TYPE_CO, alarmlevel: 0xFE
      delay {
         after 2, SECONDS
         send alarm.get, alarmtype: AlarmReportV2.ZWAVE_ALARM_TYPE_CO, notificationtype: 0x99, event: 0x99
      }
   }

   //**********************************************************************************
   //Smoke and CO REFLEXES
   //**********************************************************************************

   match reflex {
      // The device sends a heartbeat message when batteries are inserted (even if smoke is present) and once per hour
      // iff there is no smoke present. If there is still smoke present after the batteries are inserted then the
      // device will send another smoke alert 5 seconds after the heartbeat.
      on alarm.report, alarmtype: 0x0D
      set Smoke.smoke, Smoke.SMOKE_SAFE
      set CarbonMonoxide.co, CarbonMonoxide.CO_SAFE
   }

   match reflex {
      // The device sends a test alarm when the user performs the testing procedure, but only if smoke
      // is not currently detected (otherwise the button functions as a silence button and a smoke alarm
      // with level clear will be sent instead).
      on alarm.report, alarmtype: 0x0C
      set Smoke.smoke, Smoke.SMOKE_SAFE
      set CarbonMonoxide.co, CarbonMonoxide.CO_SAFE
      forward
   }

   //**********************************************************************************
   //CO EOL REFLEXES
   //**********************************************************************************
   match reflex {
      on alarm.report, alarmtype: CO_EOL_ALARM, alarmlevel: AlarmReportV2.ZWAVE_ALARM_STATUS_ON
      set CarbonMonoxide.eol, CarbonMonoxide.EOL_EOL
   }

   match reflex {
      on alarm.report, alarmtype: CO_EOL_ALARM, alarmlevel: AlarmReportV2.ZWAVE_ALARM_STATUS_OFF
      set CarbonMonoxide.eol, CarbonMonoxide.EOL_OK
   }
